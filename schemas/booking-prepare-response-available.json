{
  "description": "Schema of a booking preparation.", 
  "title": "Booking Prepare Available", 
  "required": [
    "status", 
    "checkin_date", 
    "checkout_date", 
    "partner_reference", 
    "room_type", 
    "customer_support"
  ], 
  "$schema": "http://json-schema.org/schema#", 
  "type": "object", 
  "properties": {
    "status": {
      "enum": [
        "ADJUSTED", 
        "AVAILABLE", 
        "SOLD_OUT"
      ], 
      "example": "AVAILABLE"
    }, 
    "partner_reference": {
      "type": "string", 
      "description": "Advertiser's unique hotel id.", 
      "example": "abc123"
    }, 
    "customer_support": {
      "$ref": "customer-support.json"
    }, 
    "checkin_date": {
      "description": "The user's check-in date in YYYY-MM-DD.", 
      "type": "string", 
      "example": "2016-01-25", 
      "format": "date"
    }, 
    "checkout_date": {
      "description": "The user's check-out date in YYYY-MM-DD.", 
      "type": "string", 
      "example": "2016-01-26", 
      "format": "date"
    }, 
    "room_type": {
      "$ref": "room-type.json"
    }
  }
}