{
  "title": "Hotel object",
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "required": [
    "item_id",
    "partner_reference"
  ],
  "description": "Schema of an hotel.",
  "properties": {
    "item_id": {
      "type": "integer",
      "example": 1,
      "description": "trivago item id"
    },
    "partner_reference": {
      "type": "string",
      "example": "1",
      "description": "Advertiser's unique partner reference"
    }
  }
}