{
  "$schema": "http://json-schema.org/schema#",
  "description": "Schema of an contact.",
  "type": "object",
  "required": [
    "contact",
    "description"
  ],
  "title": "Contact object",
  "properties": {
    "description": {
      "description": "Description of this contact.",
      "type": "string",
      "example": "Phone"
    },
    "contact": {
      "description": "Contact details (e.g. phone number or e-mail address).",
      "type": "string",
      "example": "444444444"
    }
  }
}