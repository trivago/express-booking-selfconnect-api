{
  "$schema": "http://json-schema.org/schema#", 
  "required": [
    "holder_name", 
    "iban", 
    "type"
  ], 
  "type": "object", 
  "properties": {
    "iban": {
      "type": "string", 
      "description": "IBAN of bank account.", 
      "example": "DE27100777770209299700"
    }, 
    "holder_name": {
      "type": "string", 
      "description": "Name of account holder.", 
      "example": "Christopher Palmer"
    }, 
    "type": {
      "enum": [
        "BankAccount"
      ], 
      "example": "BankAccount"
    }
  }
}